<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
      <%= render 'layouts/rails_default' %>
      <%= render 'layouts/shim' %>
    <%= javascript_importmap_tags %>
  </head>
<body>
    <%= render 'layouts/header' %>
    <div class="container">
      <% flash.each do |message_type, message| %>
      <%= content_tag(:div, message, class: "alert alert-#{message_type}") %>
      <% end %>
      <%= yield %>
      <div id="map-container">
        <!--<iframe id="map-canvas"-->
        <!-- src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBAtvH3oVz83MLOgVr_wuiK3VgsIw-gKWA&q=9200 University Blvd, Charleston SC 29406"-->
        <!-- allowfullscreen>-->
        <!--</iframe>-->
        <div id="map"></div>
      </div>
      <script async
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBAtvH3oVz83MLOgVr_wuiK3VgsIw-gKWA&q&callback=initMap"></script>
      <script src="../mapidx.js"></script>
      <%= render 'layouts/footer' %>
      <!--<%= debug(params) if Rails.env.development? %>-->
    </div>
  </body>
</html>